{
  "expo": {
    "name": "Home Lens",
    "slug": "homelens",
    "jsEngine": "hermes",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": "1.0.2",
    "updates": {
      "url": "https://u.expo.dev/3f70d590-b1b6-4988-90de-b0fc297eab9b"
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber": "3",
      "googleServicesFile": "./firebase/GoogleService-Info.plist",
      "bundleIdentifier": "com.homelens.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode": "resize",
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./firebase/google-services.json",
      "package": "com.homelens.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3f70d590-b1b6-4988-90de-b0fc297eab9b"
      },
      "dev": false
    },
    "plugins": [
      "@react-native-firebase/app",
      "./plugins/firebase-modular-headers",
      "./plugins/android-proguard-config",
      "./plugins/crashlytics-gradle"
    ]
  }
}
